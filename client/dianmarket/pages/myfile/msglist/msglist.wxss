/**index.wxss**/
.container {
  padding: 10px 0px;
  
}

.row-wraparrow
{
  width:95%;  
  height:75rpx;
  line-height: 75rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  flex-direction: row;
  font-size: 11pt;
  margin-left: 25rpx;
  margin-right: 25rpx;
  background: url(https://www.2dian.com/xcx/static/img/arrow-right.png) no-repeat right;
  background-size:20rpx 30rpx;
}

.row-wraparrow .itemimg
{
  border-radius: 5rpx; 
  width: 30rpx;
  height: 30rpx;
  text-align: right;
  padding-top: 0rpx;
  padding-right: 5rpx;
  margin-top:18rpx;
}



.row-wraparrowBtn:after
{
   border: none;
}

.row-line 
{
  width:100%;
  border-bottom: 1rpx solid #eee;
  margin-left: 30rpx;
}

.row-wraparrow .label-right
{
  flex: 1;
  height: 88rpx;
  line-height: 88rpx;
  text-align: left;
  margin-left: 50rpx;
  font-size: 32rpx;
}